Block(JumpLoop(
  Execute(
    Set(
      10000
      0
      Get(
        4000
        0
      )
    )
    Set(
      10000
      1
      TimeToScaledTime(Get(
        10000
        0
      ))
    )
    Set(
      10000
      2
      Add(
        Get(
          10000
          1
        )
        1
      )
    )
    Break(
      1
      Get(
        10000
        2
      )
    )
    1
  )
  0
))