0:
  10000[0] <- RuntimeUpdate[2]
  goto 1 if Greater(10000[0], 0.0) else 2
1:
  10000[0] <- TutorialMemory[0]
  TutorialMemory[0] <- Add(10000[0], 1.0)
  10000[0] <- TutorialMemory[0]
  TutorialMemory[0] <- Mod(10000[0], 7.0)
  10000[0] <- RuntimeUpdate[0]
  TutorialMemory[1] <- 10000[0]
  goto 3
2:
  10000[0] <- TutorialMemory[0]
  TutorialMemory[0] <- Subtract(10000[0], 1.0)
  10000[0] <- TutorialMemory[0]
  TutorialMemory[0] <- Mod(10000[0], 7.0)
  10000[0] <- RuntimeUpdate[0]
  TutorialMemory[1] <- 10000[0]
  goto 3
3:
  goto exit
