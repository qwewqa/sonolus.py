0:
  10000[0] <- RuntimeUpdate[2] > 0
  goto 1 if 10000[0] else 2
1:
  10000[0] <- TutorialMemory[0]
  10000[0] <- 10000[0] + 1.0
  TutorialMemory[0] <- 10000[0]
  10000[0] <- TutorialMemory[0]
  10000[0] <- 10000[0] % 7.0
  TutorialMemory[0] <- 10000[0]
  TutorialMemory[1] <- RuntimeUpdate[0]
  goto 3
2:
  10000[0] <- TutorialMemory[0]
  10000[0] <- 10000[0] - 1.0
  TutorialMemory[0] <- 10000[0]
  10000[0] <- TutorialMemory[0]
  10000[0] <- 10000[0] % 7.0
  TutorialMemory[0] <- 10000[0]
  TutorialMemory[1] <- RuntimeUpdate[0]
  goto 3
3:
  goto exit
